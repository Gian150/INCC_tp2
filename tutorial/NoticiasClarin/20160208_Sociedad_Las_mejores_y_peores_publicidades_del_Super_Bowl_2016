    var playerInstance = jwplayer(mediaPlayerId);

    var setup = {
        title: 'Super Bowl 2016',
        displaytitle: false,
        id: mediaPlayerId,
        flashplayer: 'http://www.clarin.com/static/CLAClarinV3/js/jwplayer7.5.2/jwplayer.flash.swf',
        base: 'http://www.clarin.com/static/CLAClarinV3/js/jwplayer7.5.2/',
