        height: "281",
        autostart: "",
        plugins: {},
        ga: {},
        events: {}
    }

    setup.localization = { loadingAd: 'Cargando Publicidad' };

    var JWPObjectEvents = {
        nameEvent:  "",
        videoId:    'CLAVID20160208_0018',
        videoTitle: setup.title,
        cb:         function() {}
    };

    if((navigator.userAgent.indexOf('iPod') != -1) || (navigator.userAgent.indexOf('iPad') != -1)) {
        setup.file = "http://contenidos2.clarin.com/sincro/diario/flv/2016/02/08/VID/CLAVID20160208_0018/video_iphone.mp4";
    }

    setup.related = {
        heading: "Videos Relacionados",
        file: "/rss/video/section/7/",
        onclick: "play",
        oncomplete: 'autoplay',
        autoplaytimer: 3,
        autoplaymessage: '__title__ comenzará en xx segundos!'
    }

    setup.sharing = {
        "heading": 'Comparte este video',
        "link": "http://www.clarin.com/sociedad/Super-Bowl_3_1519078095.html",
        "code": encodeURI("<iframe src='http://www.clarin.com/embed/video_CLAVID20160208_0018.html' height='261' width='424' scrolling='no' allowfullscreen='true'> </iframe>")
    }

    

    setup.advertising = {
        client: 'vast',
        schedule: {
            adpreroll: {
                tag: document.location.protocol + '//ww619.smartadserver.com/ac?siteid='+sas_site_id+'&pgid='+sas_pageVideo_id+'&fmtid=30973&ab=1&tgt=&oc=1&out=vast&ps=1&pb=0&visit=S&vcn=s&tmstp=' + Math.round(new Date().getTime()/1000),
                offset: "pre"
            },
            adpostroll: {
                tag: document.location.protocol + '//ww619.smartadserver.com/ac?siteid='+sas_site_id+'&pgid='+sas_pageVideo_id+'&fmtid=30973&ab=3&tgt=&oc=1&out=vast&ps=1&pb=0&visit=S&vcn=s&tmstp=' + Math.round(new Date().getTime()/1000),
                offset: "post"
            }
        },
        cuetext: "Publicidad",
        admessage: "Su contenido comenzará en XX segundos",
        skipoffset: 3,
        skipmessage: "Podrás saltar la publicidad en XX segundos",
        skiptext: "Saltar"
    }

    

    playerInstance.on('play', function(event){
        if (!varModIdReprod_CLAVID20160208_0018){
            //getBBTStats_CLAVID20160208_0018();
            varModIdReprod_CLAVID20160208_0018 = true;
        }

        // no hay un FIRSTPLAY event, lo individuamos con los segundos
        pos = playerInstance.getPosition();
        if (pos < 1) {
            JWPObjectEvents.eventName   = "video-firstPlay";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        } else if(eventObject.oldstate == "PAUSED") {
            JWPObjectEvents.eventName   = "video-playerPlay";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        }
    });

    playerInstance.on('pause', function(event){
        if (event.oldstate == "PLAYING" && event.newstate == "PAUSED") {
            JWPObjectEvents.eventName   = "video-playerPaused";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        }
    });

    playerInstance.on('complete', function(){
        JWPObjectEvents.eventName   = "video-playerPlayEnd";
        sendEmbeddedVideoEvents(JWPObjectEvents);
    });

    playerInstance.setup(setup);

    if (typeof JW6SS !== 'undefined') {
        JW6SS.observe(playerInstance);
    }



La bebida Kickstart de Mountain Dew, que combina jugo y cafeína. Leé también: El aviso hot que censuraron de la tanda del Super Bowl En la noche más grande la liga de fútbol americano y la publicidad, los autos marcaron la presencia de siempre y Audi hizo un buen trabajo mostrando a un astronauta retirado y deprimido que recupera la alegría de vivir manejando un Audi R8 con su hijo y David Bowie de acompañante desde la banda sonora. Pero poco se dijo en las redes de este comercial que muchos catalogaron como "bueno, pero nada más". En cambio, Hyundai -que optó por el humor y subió sus avisos a YouTube antes del SuperBowl- se ganó un lugar entre los mejores, según las menciones que tuvo en Twitter: 

