        width: "472",
        height: "281",
        autostart: "",
        plugins: {},
        ga: {},
        events: {}
    }

    setup.localization = { loadingAd: 'Cargando Publicidad' };

    var JWPObjectEvents = {
        nameEvent:  "",
        videoId:    'CLAVID20160223_0029',
        videoTitle: setup.title,
        cb:         function() {}
    };

    if((navigator.userAgent.indexOf('iPod') != -1) || (navigator.userAgent.indexOf('iPad') != -1)) {
        setup.file = "http://contenidos2.clarin.com/sincro/diario/flv/2016/02/23/VID/CLAVID20160223_0029/video_iphone.mp4";
    }

    setup.related = {
        heading: "Videos Relacionados",
        file: "/rss/video/section/2/",
        onclick: "play",
        oncomplete: 'autoplay',
        autoplaytimer: 3,
        autoplaymessage: '__title__ comenzará en xx segundos!'
    }

    setup.sharing = {
        "heading": 'Comparte este video',
        "link": "http://www.clarin.com/politica/Detencion-Shakira_3_1528077206.html",
        "code": encodeURI("<iframe src='http://www.clarin.com/embed/video_CLAVID20160223_0029.html' height='261' width='424' scrolling='no' allowfullscreen='true'> </iframe>")
    }

    

    setup.advertising = {
        client: 'vast',
        schedule: {
            adpreroll: {
                tag: document.location.protocol + '//ww619.smartadserver.com/ac?siteid='+sas_site_id+'&pgid='+sas_pageVideo_id+'&fmtid=30973&ab=1&tgt=&oc=1&out=vast&ps=1&pb=0&visit=S&vcn=s&tmstp=' + Math.round(new Date().getTime()/1000),
                offset: "pre"
            },
