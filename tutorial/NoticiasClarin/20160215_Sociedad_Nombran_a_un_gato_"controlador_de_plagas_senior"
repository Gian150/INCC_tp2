    setup.localization = { loadingAd: 'Cargando Publicidad' };

    var JWPObjectEvents = {
        nameEvent:  "",
        videoId:    'CLAVID20160215_0013',
        videoTitle: setup.title,
        cb:         function() {}
    };

    if((navigator.userAgent.indexOf('iPod') != -1) || (navigator.userAgent.indexOf('iPad') != -1)) {
        setup.file = "http://contenidos2.clarin.com/sincro/diario/flv/2016/02/15/VID/CLAVID20160215_0013/video_iphone.mp4";
    }

    setup.related = {
        heading: "Videos Relacionados",
        file: "/rss/video/section/7/",
        onclick: "play",
        oncomplete: 'autoplay',
        autoplaytimer: 3,
        autoplaymessage: '__title__ comenzará en xx segundos!'
    }

    setup.sharing = {
        "heading": 'Comparte este video',
        "link": "http://www.clarin.com/sociedad/Felix-gato-ascendieron-hacer-trabajo_3_1523277670.html",
        "code": encodeURI("<iframe src='http://www.clarin.com/embed/video_CLAVID20160215_0013.html' height='261' width='424' scrolling='no' allowfullscreen='true'> </iframe>")
    }

    

    setup.advertising = {
        client: 'vast',
        schedule: {
            adpreroll: {
                tag: document.location.protocol + '//ww619.smartadserver.com/ac?siteid='+sas_site_id+'&pgid='+sas_pageVideo_id+'&fmtid=30973&ab=1&tgt=&oc=1&out=vast&ps=1&pb=0&visit=S&vcn=s&tmstp=' + Math.round(new Date().getTime()/1000),
                offset: "pre"
            },
            adpostroll: {
                tag: document.location.protocol + '//ww619.smartadserver.com/ac?siteid='+sas_site_id+'&pgid='+sas_pageVideo_id+'&fmtid=30973&ab=3&tgt=&oc=1&out=vast&ps=1&pb=0&visit=S&vcn=s&tmstp=' + Math.round(new Date().getTime()/1000),
                offset: "post"
            }
        },
        cuetext: "Publicidad",
        admessage: "Su contenido comenzará en XX segundos",
        skipoffset: 3,
        skipmessage: "Podrás saltar la publicidad en XX segundos",
        skiptext: "Saltar"
    }

    

    playerInstance.on('play', function(event){
        if (!varModIdReprod_CLAVID20160215_0013){
            //getBBTStats_CLAVID20160215_0013();
            varModIdReprod_CLAVID20160215_0013 = true;
        }

        // no hay un FIRSTPLAY event, lo individuamos con los segundos
        pos = playerInstance.getPosition();
        if (pos < 1) {
            JWPObjectEvents.eventName   = "video-firstPlay";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        } else if(eventObject.oldstate == "PAUSED") {
            JWPObjectEvents.eventName   = "video-playerPlay";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        }
    });

    playerInstance.on('pause', function(event){
        if (event.oldstate == "PLAYING" && event.newstate == "PAUSED") {
            JWPObjectEvents.eventName   = "video-playerPaused";
            sendEmbeddedVideoEvents(JWPObjectEvents);
        }
    });

    playerInstance.on('complete', function(){
        JWPObjectEvents.eventName   = "video-playerPlayEnd";
        sendEmbeddedVideoEvents(JWPObjectEvents);
    });

    playerInstance.setup(setup);

    if (typeof JW6SS !== 'undefined') {
        JW6SS.observe(playerInstance);
    }



Félix tenía 9 meses cuando fue su primer día de trabajo y el objetivo, que imarcaría su determinación, era exterminar las ratas y palomas de esa estación. Su desempeño profesional y su destreza hicieron que se ganara en tiempo récord el corazón de sus compañeros de trabajo humanos. Los usuarios de los trenes que salen de Yorkshire del Oeste aplaudieron el gesto hacia Félix y el felino ya se convirtió en toda una celebridad. Tiene su propio perfil de Facebook, donde lo siguen más de 46.000 personas. 

